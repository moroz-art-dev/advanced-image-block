{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "advanced-image-block/advanced-image-block",
  "version": "0.1.0",
  "title": "Advanced Image Block",
  "category": "theme",
  "icon": "smiley",
  "description": "An image upload, title text and a setting for the title: level (from h1 to h6) and text colour.",
  "supports": {
    "html": false
  },
  "attributes": {
    "message": {
      "type": "string",
      "source": "text"
    },
    "color": {
      "type": "string",
      "default": "#000"
    },
    "size": {
      "type": "string",
      "default": "h6"
    },
    "image": {
      "type": "object"
    },
    "image_old": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "src"
    }
  },
  "textdomain": "advanced-image-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}